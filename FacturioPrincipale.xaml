<Window x:Class="Facturio.FacturioPrincipale"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tabGabaritsUC="clr-namespace:Facturio.Gabarits.View"
        xmlns:ucProduits="clr-namespace:Facturio.Produits"
        xmlns:ucClients="clr-namespace:Facturio.Clients"
        xmlns:ucRapports="clr-namespace:Facturio.Rapport"
        mc:Ignorable="d"
        Title="Facturio Inc." Height="500" MinHeight="500" Width="900" MinWidth="750">
    <Grid>
        <TabControl>
            <TabItem Header="Gabarits">
                <tabGabaritsUC:GabaritsTabUserControl />
            </TabItem>
            <TabItem Header="Création">
                <!--<tabGabaritsUC:GenerateurGabaritsUserControl />-->
            </TabItem>
            <TabItem Header="Produits">
                <ucProduits:ProduitUserControl />
            </TabItem>
            <TabItem Header="Clients">
                <ucClients:ClientsUserControl />
            </TabItem>
            <TabItem Header="Rapports">
                <ucRapports:RapportUserControle />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
